{
  "name": "simple-wappalyzer",
  "description": "A simple way to interacting with Wappalyzer.",
  "homepage": "https://github.com/Kikobeats/simple-wappalyzer",
  "version": "1.1.74",
  "main": "src/index.js",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://kikobeats.com"
  },
  "contributors": [
    {
      "name": "BigFoxMedia",
      "email": "ediklainer@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kikobeats/simple-wappalyzer.git"
  },
  "bugs": {
    "url": "https://github.com/Kikobeats/simple-wappalyzer/issues"
  },
  "keywords": [
    "Wappalyzer",
    "analyzer",
    "static"
  ],
  "dependencies": {
    "got": "~11.8.6",
    "jsdom": "~26.1.0",
    "lodash": "~4.17.21",
    "tough-cookie": "~5.1.0",
    "wappalyzer-core": "~6.10.66",
    "write-json-file": "~4.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@ksmithut/prettier-standard": "latest",
    "ava": "latest",
    "browserless": "latest",
    "c8": "latest",
    "ci-publish": "latest",
    "filenamify-url": "2",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "github-generate-release": "latest",
    "html-get": "latest",
    "humanize-url": "2",
    "nano-staged": "latest",
    "simple-git-hooks": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest"
  },
  "engines": {
    "node": ">= 10"
  },
  "files": [
    "scripts",
    "src"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "contributors": "(npx git-authors-cli && npx finepack && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "lint": "standard-markdown README.md && standard",
    "postinstall": "node scripts/postinstall",
    "postrelease": "npm run release:tags && npm run release:github && (ci-publish || npm publish --access=public)",
    "pretest": "npm run lint",
    "release": "standard-version -a",
    "release:github": "github-generate-release",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "test": "c8 ava"
  },
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0
      ]
    }
  },
  "nano-staged": {
    "*.js": [
      "prettier-standard",
      "standard --fix"
    ],
    "*.md": [
      "standard-markdown"
    ],
    "package.json": [
      "finepack"
    ]
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit",
    "pre-commit": "npx nano-staged"
  }
}
